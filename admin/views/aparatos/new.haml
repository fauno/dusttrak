%ul.nav.nav-tabs
  %li=link_to tag_icon(:list, pat(:list)), url(:aparatos, :index)
  %li.active=link_to tag_icon(:plus, pat(:new)), url(:aparatos, :new)

.tabs-content
  -form_for :aparato, url(:aparatos, :create), :class => 'form-horizontal' do |f|
    =partial 'aparatos/form', :locals => { :f => f }
